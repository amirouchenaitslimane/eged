        <!-- formulaire ajout de frais -->
        <div class="modal fade" id="addFraisModal" data-backdrop="static" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
            <div class="modal-dialog modal-lg" role="document">
                <div class="modal-content">
                    <div class="modal-header">
                        <h5 class="modal-title" id="exampleModalLabel">Ajout frais</h5>
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                            <span aria-hidden="true">&times;</span>
                        </button>
                    </div>
                    <div class="modal-body">
                        <form>

                            <div class="row">
                                <div class="col-sm-6">
                                    <label for="recipient-name" class="col-form-label">Date *</label>
                                    <input type="text" class="form-control" id="recipient-name">
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-sm-6">
                                    <label for="message-text" class="col-form-label">Libelle frais *</label>
                                    <input type="text" class="form-control" >
                                </div>

                                <div class="col-sm-6">
                                    <label for="message-text" class="col-form-label">Type *</label>
                                    <select id="inputState" class="form-control">
                                        <option>Choix type frais</option>
                                        <option>Repas personnel</option>
                                        <option>Repas professionnel</option>
                                        <option>Transport</option>
                                        <option>Achat professionnel</option>
                                        <option>Cadeau client</option>
                                    </select>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-sm-6">
                                    <label for="message-text" class="col-form-label">Montant TTC *</label>
                                    <input type="text" class="form-control">
                                </div>

                                <div class="col-sm-6">
                                    <label for="message-text" class="col-form-label">Taxe *</label>
                                    <input type="text" class="form-control">
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-sm-6">
                                    <label for="message-text" class="col-form-label">Montant HT *</label>
                                    <input type="text" class="form-control">
                                </div>

                                <div class="col-sm-6">
                                    <label for="message-text" class="col-form-label">Justificatif *</label>
                                    <input type="text" class="form-control">
                                </div>
                            </div>
                        </form>
                    </div>

